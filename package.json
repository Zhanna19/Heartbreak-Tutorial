{
  "name": "heartbeat",
  "description": "A Vue.js project",
  "version": "1.0.0",
  "author": "Zhanna19 <funnygirl322@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --inline --hot --host=localhost",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "dist": "build --tasks win-x86,win-x64,linux-x86,linux-x64,mac-x64 --mirror https://dl.nwjs.io/ .",
    "start": "run --x86 --mirror https://dl.nwjs.io/ .",
    "nwdev": "concurrently --kill-others --raw \"npm run dev \" \".\\node_modules\\.bin\\run .\\launch\""
  },
  "dependencies": {
    "concurrently": "^3.6.0",
    "fs": "0.0.1-security",
    "nwjs-builder-phoenix": "^1.14.6",
    "ping-monitor": "^0.1.6",
    "vue": "^2.5.3",
    "vue-router": "^3.0.1",
    "vuetify": "^1.0.0",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-2": "^6.24.1",
    "babel-plugin-transform-imports": "^1.4.1",
    "cross-env": "^3.0.0",
    "css-loader": "^0.25.0",
    "file-loader": "^0.9.0",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "vue-loader": "^12.1.0",
    "vue-template-compiler": "^2.5.3",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  },
  "build": {
    "nwVersion": "0.31.4"
  },
  "main": "index.html"
}
